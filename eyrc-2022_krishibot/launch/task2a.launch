<?xml version="1.0" ?>
<!-- ================================ DO NOT EDIT THIS FILE ===================================== -->
<launch>
  <arg name="record" default="false"/>
	<arg name="duration" default="150"/>
	<arg name="rec_name" default="KB_TASK2A.bag"/>

<!-- Recording section or Rosbag node --> 
	<group if="$(arg record)">
		<node name="state_node1" pkg="topic_tools" type="throttle" args="messages /gazebo/model_states 10.0"/>
    <node name="state_node2" pkg="topic_tools" type="throttle" args="messages /gazebo/link_states 10.0"/>
		<node name="rosbag_record_T2" pkg="rosbag" type="record"
		   args="record -O $(find eyrc-2022_krishibot)/bag_files/$(arg rec_name) --duration=$(arg duration) /gazebo/model_states_throttle /gazebo/link_states_throttle /sysInfo" output="screen"/>
	</group>


  <!--  agribot with ur5 -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find eyrc-2022_krishibot)/models/ebot/agri_bot.xacro'"/>
  <!-- Spawn the robot model -->
  <arg name="x" default="0"/>
  <arg name="y" default="0"/>
  <arg name="z" default="0.14"/>
  <arg name="yaw" default="0"/>
  

  <!-- Spawn the robot model -->
  <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" 
      args="-urdf -param robot_description -model ebot -x $(arg x) -y $(arg y) -z $(arg z) -Y $(arg yaw)" />

  <node pkg="gazebo_ros_link_attacher" name="ebot_stationary" type="attach.py" output="screen"/>
  
    <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- <arg name="world_name" value="worlds/mud.world"/> Note: the world_name  is with respect to GAZEBO_RESOURCE_PATH environmental variable -->
    <arg name="world_name" value="$(find eyrc-2022_krishibot)/worlds/task2a.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="physics" value="ode"/>
    <arg name="recording" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <!-- Executing Exe file -->
  <node pkg="eyrc-2022_krishibot" type="task2aEx" name="exeFileT0" output="screen" />

  <include file="$(find ur5_moveit)/launch/ur5_bringup_moveit.launch"/> 
  <node name="picknplace" pkg="eyrc-2022_krishibot" type="maniStack.py" output="screen"/>         

</launch>
<!-- ======================================================================================= -->
