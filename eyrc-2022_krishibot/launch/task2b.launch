<?xml version="1.0" ?>
<!-- ================================ DO NOT EDIT THIS FILE ===================================== -->
<launch>
  <arg name="record" default="false"/>
	<arg name="duration" default="150"/>
	<arg name="rec_name" default="KB_TASK2B.bag"/>

<!-- Recording section or Rosbag node --> 
	<group if="$(arg record)">
		<node name="rosbag_record_T2" pkg="rosbag" type="record"
		   args="record -O $(find eyrc-2022_krishibot)/bag_files/$(arg rec_name) --duration=$(arg duration) /center_rgb /center_depth /sysInfo" output="screen"/>
	</group>
  

  <!-- Executing Exe file -->
  <node pkg="eyrc-2022_krishibot" type="task2bEx" name="exeFileT2b" output="screen" />

  <!-- <include file="$(find ur5_moveit)/launch/ur5_bringup_moveit.launch"/> -->
  <node name="perception_script" pkg="eyrc-2022_krishibot" type="percepStack.py" output="screen"/>          

</launch>
<!-- ======================================================================================= -->